{
    "manifest_version": 3,
    "name": "AUTO-MFA",
    "description": "auto mfa (Multi Factor Authentication)",
    "version": "0.0.0.3",
    "author": "ydq",
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "contextMenus",
        "storage",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    }
}